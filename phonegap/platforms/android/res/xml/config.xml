<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nextgendriver.project" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Next Gen Driver</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/prepareAngular2App.js" type="before_prepare" />
    <icon density="ldpi" src="images_res/res/icon/android/drawable-ldpi-icon.png" />
    <icon density="mdpi" src="images_res/res/icon/android/drawable-mdpi-icon.png" />
    <icon density="hdpi" src="images_res/res/icon/android/drawable-hdpi-icon.png" />
    <icon density="xhdpi" src="images_res/res/icon/android/drawable-xhdpi-icon.png" />
    <icon density="xxhdpi" src="images_res/res/icon/android/drawable-xxhdpi-icon.png" />
    <icon density="xxxhdpi" src="images_res/res/icon/android/drawable-xxxhdpi-icon.png" />
    <splash density="land-ldpi" src="images_res/res/screen/android/drawable-land-ldpi-screen.png" />
    <splash density="land-mdpi" src="images_res/res/screen/android/drawable-land-mdpi-screen.png" />
    <splash density="land-hdpi" src="images_res/res/screen/android/drawable-land-hdpi-screen.png" />
    <splash density="land-xhdpi" src="images_res/res/screen/android/drawable-land-xhdpi-screen.png" />
    <splash density="land-xxhdpi" src="images_res/res/screen/android/drawable-land-xxhdpi-screen.png" />
    <splash density="land-xxxhdpi" src="images_res/res/screen/android/drawable-land-xxxhdpi-screen.png" />
    <splash density="port-ldpi" src="images_res/res/screen/android/drawable-port-ldpi-screen.png" />
    <splash density="port-mdpi" src="images_res/res/screen/android/drawable-port-mdpi-screen.png" />
    <splash density="port-hdpi" src="images_res/res/screen/android/drawable-port-hdpi-screen.png" />
    <splash density="port-xhdpi" src="images_res/res/screen/android/drawable-port-xhdpi-screen.png" />
    <splash density="port-xxhdpi" src="images_res/res/screen/android/drawable-port-xxhdpi-screen.png" />
    <splash density="port-xxxhdpi" src="images_res/res/screen/android/drawable-port-xxxhdpi-screen.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
